{
  "name": "@icose/vite-plugin-fmwv",
  "version": "1.0.0",
  "description": "This is a Vite plugin that allows you to mock the FileMaker Web Viewer environment in your Vite project.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite --config ./tests/vite.config.ts  --host 0.0.0.0",
    "build": "tsup"
  },
  "keywords": [
    "vite-plugin",
    "filemaker"
  ],
  "files": [
    "dist"
  ],
  "author": "Alexandros Raikos",
  "license": "MIT",
  "homepage": "https://github.com/icoseuk/vite-plugin-fmwv#readme",
  "devDependencies": {
    "@types/node": "^22.5.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}